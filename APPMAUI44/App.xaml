<?xml version = "1.0" encoding = "UTF-8" ?>
<Application xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:APPMAUI44"
             xmlns:converters="clr-namespace:APPMAUI44.Converters"
             x:Class="APPMAUI44.App">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/Colors.xaml" />
                <ResourceDictionary Source="Resources/Styles/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <converters:IsTrainerToColorConverter x:Key="IsTrainerToColorConverter"/>
            <converters:DummyConverter x:Key="DummyConverter"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>

